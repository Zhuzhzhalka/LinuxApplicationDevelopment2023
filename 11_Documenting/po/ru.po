#: ../src/num_guesser.c:85
#, c-format
msgid ""
"Usage: ./num_guesser [OPTION]...\n"
msgstr ""
"Использование: ./num_guesser [ОПЦИЯ]...\n"

#: ../src/num_guesser.c:86
#, c-format
msgid ""
"Simple guess number game.\n\n"
msgstr ""
"Простая игра на загадывание чисел.\n\n"

#: ../src/num_guesser.c:87
#, c-format
msgid ""
"  -h, --help    display help message\n"
msgstr ""
"  -h, --help    вывести справочное сообщение\n"

#: ../src/num_guesser.c:88
#, c-format
msgid ""
"  -r            work with numbers in roman form\n"
msgstr ""
"  -r            работать с римскими цифрами\n"

#: ../src/num_guesser.c:157
#, c-format
msgid ""
"Think about some number from I to C. I'll guess it!\n"
msgstr ""
"Загадайте какое-то число от I до C. Я его угадаю!\n"

#: ../src/num_guesser.c:159
#, c-format
msgid ""
"Think about some number from 1 to 100. I'll guess it!\n"
msgstr ""
"Загадайте какое-то число от 1 до 100. Я его угадаю!\n"

#: ../src/num_guesser.c:110
#, c-format
msgid ""
"Is your number bigger than %s?\n"
msgstr ""
"Ваше число больше чем %s?\n"

#: ../src/num_guesser.c:113
#, c-format
msgid ""
"Is your number bigger than %d?\n"
msgstr ""
"Ваше число больше чем %d?\n"

#: ../src/num_guesser.c:120
msgid "yes"
msgstr "да"

#: ../src/num_guesser.c:122
msgid "no"
msgstr "нет"

#: ../src/num_guesser.c:125
#, c-format
msgid ""
"Answer 'yes' or 'no'\n"
msgstr ""
"Ответьте 'да' или 'нет'\n"

#: ../src/num_guesser.c:165
#, c-format
msgid ""
"Your number is %s\n"
msgstr ""
"Ваше число - это %s\n"

#: ../src/num_guesser.c:167
#, c-format
msgid ""
"Your number is %d\n"
msgstr ""
"Ваше число - это %d\n"
